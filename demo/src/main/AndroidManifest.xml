<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.baozi.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.baozi.demo.activity.MainActivity"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CityListActivity" />
        <activity android:name=".activity.NewsActivity" />
        <activity android:name=".activity.SortActivity" />
        <activity android:name=".activity.SwipeSortActivity" />
        <activity android:name=".activity.CartActivity" />
        <activity android:name=".activity.TBHomeListActivity" />
    </application>
</manifest>